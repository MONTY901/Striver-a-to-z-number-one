import java.util.Scanner;
public class main {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        int age = scn.nextInt();

        if(age >= 18){
            System.out.println("You are an adult");
        }
        else{
            System.out.println("You are a kid");
        }

        scn.close();
    }
}
